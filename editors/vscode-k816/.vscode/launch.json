{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run k816 Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "env": {
        "RUST_LOG": "k816_lsp=info"
      }
    }
  ]
}
